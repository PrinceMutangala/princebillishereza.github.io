    <script type="text/javascript">
    $(document).ready(function(){

      setAjax();
      
      $.timeago.settings.lang = 'fr';
      $("#timeago").timeago();

      getJobList();

      var _search=document.querySelector('#search');
      
      if(_search!==null){
        _search.addEventListener('keyup', debounce_(function(){
         fetch_data("/fetch_jobposts?page=",1,'#jobLists',_search.value);
      },400));   
      }

     

      $(document).on('click','.view_jobpost',function(){

        var slug=$(this).attr('id')
        window.location.href='/job_post/'+slug
      })
      .on('click','.view_service',function(){

          var slug=$(this).attr('id')
          window.location.href='/single_service/'+slug
      })

      .on('click','.subscribeBtn',function(e){
        e.preventDefault();
     
        var email=$('#email_s').val();
         if ( ValidateEmail(email)==false){
            $('#successMessage').hide();
            $('#errorMessage3').show().text('Invalid email adress');
            console.log('error')
            return false;    
        } 
        $.ajax({
            url:'/subscribes',
            method:'post',
            data:{email:email},
            dataType:'json',
            beforeSend:function(){
              $('.subscribeBtn').attr('disabled',true);
            },
            success:function(data){
              $('#email_s').val('')
              $('.subscribeBtn').attr('disabled',false);
              $('#errorMessage3').hide();
              $('#successMessage').show().text(data.message);
            },
            error:function(error){
              $('.subscribeBtn').attr('disabled',false);
              $('#errorMessage3').show().text('check your internet connection !');
            }
        })
    })
    .on('click', '#pagination nav a', function(event){
      event.preventDefault(); 
       var page = $(this).attr('href').split('page=')[1];
       $('#hidden_page').val(page);
       var search=$('#search').val();
       fetch_data("/fetch_jobposts?page=",page,'#jobLists',search);
    });



    
    function getJobList(){
      fetch_data("/fetch_jobposts?page=",1,'#jobLists','');
    }

  function skeleton_loader(){
      var loader='';
      loader+='<div class="ph-item">';
      loader+='<div  class="ph-row"><div class="php-col-12 big"></div>';
      for (var i = 0; i < 20; i++) {
        loader+='<div class="php-col-12"></div>';
      }
      loader+=' </div>';
      loader+=' </div>';
      return loader;
  }

    function fetch_data(url,page,el,query){
      $.ajax({
        url:url+page+"&query="+query,
        beforeSend:function(){
          $(el).html(skeleton_loader(),2000);
        },
       success:function(data){
          $(el).html('').html(data);
       }
      });
    }

    function ValidateEmail(mail) 
    {
     if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mail)){
        return true;
      }
        return false;
    }

    function debounce_(fn,delay){
      let debounceTimer
      return function(...args){
        clearTimeout(debounceTimer)
         debounceTimer=setTimeout(()=>fn(...args),delay)
      }
    }

    function setAjax()
    {
      $.ajaxSetup({
        headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
        }
     });
    }


  })
  </script>  <!-- End script start -->
</body>

</html>